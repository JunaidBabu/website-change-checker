        <script>

 $(document).ready(function(){

            $("#empdeg").change(function(){


                var deptid = $(this).val();

                $.ajax({
                    url: 'getdegree.php',
                    type: 'post',
                    data: {depart:deptid},
                    dataType: 'json',
                    success:function(response){

                        var len = response.length;

                        $("#empsdeg").empty();


                            $("#empsdeg").append("<option value=''>Select</option>");
                        for( var i = 0; i<len; i++){                            var name = response[i]['name'];


                  

                            $("#empsdeg").append("<option value='"+name+"'>"+name+"</option>");

                        }
                    }
                });
            });







$("#pid").change(function(){


                var deptid = $(this).val();
              var hcode=document.getElementById('hcode').value


                    $.ajax({
                    url: 'getpid.php',
                    type: 'post',
                    data: {depart:deptid,hcode:hcode},
                    dataType: 'json',
                    success:function(response){

                        var len = response.length;


                        for( var i = 0; i<len; i++)
{                            
					var name = response[i]['name'];
                           var hname = response[i]['hname']; 


if(name==1){
       
 alert('The ID: '+deptid+' is Reffered from '+hname+'');

}else if(name==2){

alert('The ID: '+deptid+'  Already Admitted Hospital Name: '+hname+'');
document.getElementById('pid').value='';

}

                        }
                    }
                });
            });
  


            $("#btype").change(function(){


                var deptid = $(this).val();
              var hcode=document.getElementById('hcode').value


                    $.ajax({
                    url: 'getbed.php',
                    type: 'post',
                    data: {depart:deptid,hcode:hcode},
                    dataType: 'json',
                    success:function(response){

                        var len = response.length;


                        for( var i = 0; i<len; i++)
{                            
					var name = response[i]['name'];
                             var avail = response[i]['avail'];
if(avail<=0){
       $("p").hide();                    
 alert('The selected  Bed Type is not available in your Hospital.');

}else{

$("p").show();
document.getElementById('bedinc').value=name;

}

                        }
                    }
                });
            });
  

        });
        </script> 

